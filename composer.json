{
    "name": "gpanos/explain",
    "description": "Explain shell commands",
    "keywords": [
        "shell",
        "explain"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Dimitris Karapanos",
            "email": "karapanos.dimitris@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^6.5.5",
        "illuminate/http": "^8.0"
    },
    "require-dev": {
        "laravel-zero/framework": "^8.8",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/explain"
    ]
}
